<% layout('/layout/boilerplate') %>
<body>
    <h3>create new class </h3>
    <form method="post" action="/class/<%=id%>/newstudent" class="needs-validation" novalidate>
        <div>
            <label for="name" class="form-label">Name</label>
            <input type="text" name="name" class="form-control" required>
            <div class="valid-feedback">Looks good!</div>
        </div>
    
        <div>
            <label for="rollNo" class="form-label">RollNo</label>
            <input type="Number" name="rollNo"  class="form-control" required>
            <div class="invalid-feedback">Please enter a valid rollno.</div>
        </div>
    
        <div>
            <label for="age" class="form-label">Age</label>
            <input type="Number" name="age"  class="form-control" required>
            <div class="invalid-feedback">Please enter valid age </div>
        </div>
    
        <div>
            <label for="gender" class="form-label">Gender</label>
            <input type="text" name="gender" class="form-control" required>
            <div class="invalid-feedback">Please enter valid gender.</div>
            <div class="valid-feedback">Looks good!</div>
        </div>
        <div>
            <label for="address" class="form-label">address</label>
            <input type="text" name="address" class="form-control" required>
            <div class="invalid-feedback">Please enter valid address</div>
            <div class="valid-feedback">Looks good!</div>
        </div>
        <div>
            <label for="fathername" class="form-label">fathername</label>
            <input type="text" name="fathername"  class="form-control" required>
            <div class="invalid-feedback">Please enter valid fathername</div>
            <div class="valid-feedback">Looks good!</div>
        </div>
        <div>
            <label for="mothername" class="form-label">mothername</label>
            <input type="text" name="mothername"  class="form-control" required>
            <div class="invalid-feedback">Please enter valid mothername</div>
            <div class="valid-feedback">Looks good!</div>
        </div>
        <div>
            <label for="dateOfBirth" class="form-label">dateOfBirth</label>
            <input type="text" name="dateOfBirth"  class="form-control" required>
            <div class="invalid-feedback">Please enter valid dateOfBirth</div>
            <div class="valid-feedback">Looks good!</div>
        </div>
        <div>
            <label for="parentcontact" class="form-label">parentcontact</label>
            <input type="Number" name="parentcontact"  class="form-control" required>
            <div class="invalid-feedback">Please enter valid parentcontact</div>
            <div class="valid-feedback">Looks good!</div>
        </div>
      
        <div>
            <label for="grade" class="form-label">Grade</label>
            <input type="text" name="grade"  class="form-control" required>
            <div class="invalid-feedback">Please enter valid grade </div>
            <div class="valid-feedback">Looks good!</div>
        </div>
        <div>
            <label for="username" class="form-label">Username/label>
            <input type="text" name="username"  class="form-control" required>
            <div class="invalid-feedback">Please enter valid username </div>
            <div class="valid-feedback">Looks good!</div>
        </div>
        <div>
            <label for="email" class="form-label">email</label>
            <input type="text" name="email"  class="form-control" required>
            <div class="invalid-feedback">Please enter valid email </div>
            <div class="valid-feedback">Looks good!</div>
        </div>
        <div>
            <label for="password" class="form-label">password</label>
            <input type="password" name="password"  class="form-control" required>
            <div class="invalid-feedback">Please enter valid password </div>
            <div class="valid-feedback">Looks good!</div>
        </div>
    
        <button class="btn btn-primary">Submit</button>
    </form>
    
    

</body>
